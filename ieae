[32m+[m[32m            for (KmlPlacemark placemark : container.getPlacemarks()) {[m
[32m+[m[32m                // Do something to Placemark[m
[32m+[m[32m                KmlGeometry g = placemark.getGeometry();[m
[32m+[m[32m                if (g.getGeometryType().equals("Point")) {[m
